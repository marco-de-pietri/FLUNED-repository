    face1
    {
        type            zeroGradient;
    }
    face2
    {
        type            fixedValue;
        value           uniform 0;
    }
    face3
    {
        type            zeroGradient;
    }
    face4
    {
        type            zeroGradient;
    }
    face5
    {
        type            zeroGradient;
    }
    face6
    {
        type            zeroGradient;
    }
    face7
    {
        type            zeroGradient;
    }
